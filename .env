# Application Configuration
NODE_ENV=development
PORT=3000

# YNAB Configuration (Required)
# YNAB_TOKEN=your_ynab_personal_access_token

# Investment Config File URL (Required)
# INVESTMENTS_CONFIG_FILE_URL=https://example.com/path/to/your/investments.yaml

# Market Data Provider API Keys (Optional - for better asset price coverage)
# Alpha Vantage API Key
# ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key

# CoinMarketCap API Key
# COINMARKETCAP_API_KEY=your_coinmarketcap_key

# Polygon.io API Key
# POLYGON_API_KEY=your_polygon_api_key

# Finnhub API Key
# FINNHUB_API_KEY=your_finnhub_api_key
# YNAB_BUDGET_ID=your_budget_id_for_testing
# YNAB_INVESTMENTS_ACCOUNT_ID=your_investments_account_id
# YNAB_CRYPTO_ACCOUNT_ID=your_crypto_account_id
